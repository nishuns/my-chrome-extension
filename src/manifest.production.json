{
    "manifest_version": 3,
    "name": "Dictionary",
    "description": "it will catch the selected word and add it to the stack with the meaning",
    "version": "0.0.1",
    "background": {
        "service_worker": "/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "options_page": "options.html"
}